            android:name="com.mixed_up.dictionaryv2.Show"
            android:label="@string/title_activity_show" >
        </activity>
        <activity
            android:name="com.mixed_up.dictionaryv2.Search"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name="com.mixed_up.dictionaryv2.SearchResults"
            android:label="@string/title_activity_search_results" >
